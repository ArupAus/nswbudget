{
  "name": "nswbudget",
  "version": "2017.0.0",
  "description": "Mapping the Budget 2017-18",
  "main": "webpack.config.babel.js",
  "dependencies": {
    "@turf/bbox": "^4.4.0",
    "@turf/helpers": "^4.4.0",
    "autoprefixer": "^6.6.1",
    "babel-cli": "^6.24.0",
    "babel-loader": "^6.2.10",
    "babel-plugin-styled-components": "^1.1.4",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-stage-0": "^6.22.0",
    "babel-register": "^6.22.0",
    "babel-watch": "^2.0.5",
    "copy-webpack-plugin": "^4.0.1",
    "csv-parse": "^4.6.5",
    "d3-array": "^1.2.0",
    "d3-format": "^1.2.0",
    "d3-request": "^1.0.3",
    "d3-time-format": "^2.0.3",
    "dotenv": "^4.0.0",
    "express": "^4.14.0",
    "extract-text-webpack-plugin": "^2.1.0",
    "file-loader": "^0.9.0",
    "get-root": "^0.0.2",
    "history": "^4.6.2",
    "html-webpack-plugin": "^2.26.0",
    "html-webpack-template": "^6.0.1",
    "immutability-helper": "^2.2.2",
    "keymirror": "^0.1.1",
    "leaflet": "^1.0.3",
    "lodash.map": "^4.6.0",
    "lodash.uniqby": "^4.7.0",
    "lodash.values": "^4.3.0",
    "lost": "^8.0.0",
    "postcss-flexibility": "^1.1.0",
    "postcss-nested": "^1.0.0",
    "preact": "^8.1.0",
    "preact-compat": "^3.16.0",
    "react": "^15.4.2",
    "react-dimensions": "^2.0.0-alpha1",
    "react-dom": "^15.5.4",
    "react-intl": "^2.3.0",
    "react-leaflet": "^1.2.0",
    "react-media": "^1.5.1",
    "react-redux": "^5.0.2",
    "react-router": "^4.1.1",
    "react-router-dom": "^4.1.1",
    "redux": "^3.6.0",
    "redux-devtools-extension": "^2.13.2",
    "redux-thunk": "^2.2.0",
    "reselect": "^3.0.1",
    "style-loader": "^0.13.1",
    "styletron": "^2.5.7",
    "styletron-react": "^2.5.8",
    "topojson": "^2.2.0",
    "topojson-client": "^3.0.0",
    "topojson-server": "^3.0.0",
    "topojson-simplify": "^3.0.1",
    "webpack": "^2.6.1"
  },
  "babel": {
    "presets": [
      "es2015",
      "react",
      "stage-0"
    ]
  },
  "eslintConfig": {
    "extends": "airbnb",
    "parser": "babel-eslint",
    "settings": {
      "import/resolver": "webpack"
    },
    "env": {
      "browser": true,
      "jest": true
    },
    "rules": {
      "import/no-extraneous-dependencies": [
        0
      ],
      "import/no-unresolved": [
        0
      ],
      "import/extensions": [
        0
      ],
      "linebreak-style": [
        0
      ],
      "semi": [
        0
      ],
      "no-confusing-arrow": [
        0
      ],
      "comma-dangle": [
        0
      ],
      "react/jsx-filename-extension": [
        0
      ],
      "import/no-named-as-default": [
        0
      ],
      "import/no-named-as-default-member": [
        0
      ],
      "react/prop-types": [
        0
      ],
      "max-len": [
        0
      ],
      "no-class-assign": [
        0
      ],
      "eqeqeq": [
        0
      ],
      "no-shadow": [
        0
      ],
      "quotes": [
        0
      ],
      "arrow-parens": [
        0
      ],
      "arrow-body-style": [
        0
      ],
      "no-plusplus": [
        0
      ],
      "react/prefer-stateless-function": [
        0
      ],
      "consistent-return": [
        0
      ],
      "import/prefer-default-export": [
        0
      ],
      "react/sort-comp": [
        0
      ],
      "react/no-string-refs": [
        0
      ],
      "prefer-arrow-callback": [
        0
      ],
      "no-inner-declarations": [
        0
      ],
      "no-return-assign": [
        0
      ],
      "no-underscore-dangle": [
        0
      ],
      "no-param-reassign": [
        0
      ],
      "one-var": [
        0
      ],
      "no-unused-expressions": [
        0
      ],
      "no-use-before-define": [
        0
      ],
      "react/no-find-dom-node": [
        0
      ],
      "no-extend-native": [
        0
      ],
      "jsx-a11y/no-static-element-interactions": [
        0
      ],
      "jsx-a11y/label-has-for": [
        0
      ],
      "func-names": [
        0
      ]
    }
  },
  "devDependencies": {
    "@storybook/react": "^3.0.0",
    "babel-eslint": "^7.1.1",
    "babel-jest": "^18.0.0",
    "babel-preset-react-native": "^1.9.1",
    "babel-register": "^6.18.0",
    "css-loader": "^3.0.0",
    "enzyme": "^2.8.2",
    "eslint": "^5.16.0",
    "eslint-config-airbnb": "^14.0.0",
    "eslint-import-resolver-webpack": "^0.8.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^3.0.2",
    "eslint-plugin-react": "^6.9.0",
    "jest": "^20.0.4",
    "jest-enzyme": "^3.2.0",
    "postcss-css-variables": "^0.6.0",
    "postcss-loader": "^1.2.2",
    "postcss-nested": "^1.0.0",
    "react-test-renderer": "^15.5.4"
  },
  "scripts": {
    "build": "webpack --env=prod",
    "dev": "node -r babel-register index.js",
    "test:dev": "jest --watch",
    "test": "jest --coverage",
    "lint": "eslint src",
    "storybook": "start-storybook -p 9001 -c .storybook",
    "build-storybook": "build-storybook",
    "postinstall": "npm run geo && npm run buildData",
    "geo": "npm run geo:regions && npm run geo:simplify",
    "geo:regions": "node -r babel-register scripts/addRegionToLGAs.js",
    "geo:simplify": "geo2topo lgas=tmp/lgas_simplified_w_region.json | topomerge regions=lgas -k \"d.properties.region\" | topomerge cleanlgas=lgas -k \"d.properties.lga\" | topoquantize 1e10 | toposimplify -s 1e-12 -F > tmp/lgas.json",
    "geo:debug": "topo2geo lgas=tmp/lgas_geo.json < tmp/lgas.json",
    "buildData": "node -r babel-register scripts/buildData.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ArupAus/nswbudget.git"
  },
  "author": "Liam Hanrahan <liam.hanrahan@arup.com>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/ArupAus/nswbudget/issues"
  },
  "homepage": "https://github.com/ArupAus/nswbudget#readme",
  "jest": {
    "setupTestFrameworkScriptFile": "./node_modules/jest-enzyme/lib/index.js",
    "collectCoverageFrom": [
      "src/**/*.js"
    ],
    "transform": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileTransform.js",
      "^.+\\.js$": "babel-jest"
    },
    "collectCoverage": false
  }
}
